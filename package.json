    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2",
    "yeps": "^1.1.1",
    "yeps-error": "^1.3.1",
    "yeps-server": "^1.1.2"
  }
