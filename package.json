  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evheniy/yeps-restify/issues"
  },
  "homepage": "https://github.com/evheniy/yeps-restify#readme",
  "files": [
    "index.js"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
